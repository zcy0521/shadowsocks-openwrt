{
  "log": {
    "access": "/var/log/access.log",
    "error": "/var/log/error.log",
    "loglevel": "warning"
  },
  "dns": {
    "servers": [
      {
        "address": "127.0.0.1",
        "port": 5300,
        "domains": [
          "geosite:geolocation-!cn",
          "geosite:tld-!cn",
          "geosite:google",
          "geosite:twitter",
          "geosite:netflix",
          "geosite:spotify"
        ]
      },
      {
        "address": "222.222.222.222",
        "port": 53,
        "domains": [
          "geosite:cn"
        ]
      }
    ]
  },
  "inbounds": [
    {
      "port": 5353,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1",
        "port": 5300,
        "network": "tcp,udp"
      },
      "tag": "dns-in"
    }
  ],
  "outbounds": [
    {
      "protocol": "dns",
      "tag": "dns-out"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": "dns-in",
        "outboundTag": "dns-out"
      }
    ]
  }
}